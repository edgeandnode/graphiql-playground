{
  "name": "@edgeandnode/graphiql-playground",
  "version": "1.2.1",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@8.2.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/edgeandnode/graphiql-playground.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --declaration --emitDeclarationOnly",
    "release": "pnpm build && pnpm publish",
    "build:demo": "cross-env BUILD_DEMO=1 vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "ci": "pnpm typecheck && pnpm lint",
    "test": "jest"
  },
  "dependencies": {
    "@graphiql/plugin-explorer": "0.1.12",
    "@graphiql/react": "0.15.0",
    "@graphiql/toolkit": "0.8.0"
  },
  "devDependencies": {
    "graphql": "^16",
    "@edgeandnode/components": "^24.4.2",
    "@edgeandnode/eslint-config": "^1.1.0",
    "@emotion/react": ">=11",
    "@jest/types": "^29.3.1",
    "@types/codemirror": "^5.60.6",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "@vitejs/plugin-react": "~2.0.1",
    "cross-env": "^7.0.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.31.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.3.1",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "theme-ui": "^0.15.4",
    "typescript": "^4.9.4",
    "vite": "~3.0.7"
  },
  "peerDependencies": {
    "graphql": "^16",
    "@edgeandnode/components": "^23",
    "@emotion/react": ">=11",
    "react": ">=16",
    "react-dom": ">=16",
    "theme-ui": ">=0.14"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "graphql"
      ],
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    }
  }
}
